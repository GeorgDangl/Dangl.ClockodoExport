{
  "metadata":[
    {
      "src":[
        {
          "src":"./src",
          "files":[
            "Dangl.ClockodoExport/*.csproj"
          ],
          "exclude":[
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest":"obj/api"
    }
  ],
  "build":{
    "content":[
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "output/**/*",
          "docs/**/*",
          "build/**/*",
          "GitVersion.yml",
          "**/*.nupkg"
        ]
      }
    ],
    "resource":[
      {
        "files":[
          "**.png"
        ]
      }
    ],
    "overwrite":"specs/*.md",
    "globalMetadata":{
      "_appTitle":"Dangl.ClockodoExport Documentation",
      "_appFooter":"&copy; <a href=\"https://www.dangl-it.com\" target=\"_blank\">Dangl IT GmbH</a>",
      "_appLogoPath":"app-logo.png",
      "_appFaviconPath":"app-logo.png",
      "_enableSearch":true,
      "_gitUrlPattern": "github"
    },
    "dest":"./output/docs",
    "template": [
      "default",
      "docs/template"
    ]
  }
}
